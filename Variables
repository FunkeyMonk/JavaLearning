//f5 to run
class Quickstart {
    public static void main(String[] args) {
        //
        // name variables first word lowercase, rest are capitals, called camelCase
        //

        //----------Strings------------
        //string is line of text
        String name = "Ansinged Li";
        //backslash is an escape sequence, so code will print out "Hello World"
        System.out.println("\"Hello World\"");
        //\n is escape sequence that will create new line
        System.out.println("Hello\nGoodbye");

        //-----------Other Variables--------------
        //int is one number
        int yearCreated = 1995;
        //double is number with decimal values
        double age = 19.7;
        //boolean is a true or false statement
        boolean JGIsAdult = true;
        //char is used to hold one character using single quote('')
        char FName = 'A';

        //-------------Console Printing-------------
        System.out.println(name);
        System.out.println(yearCreated);
        System.out.println(age);
        System.out.println(JGIsAdult);
        System.out.println(FName);
    }
}
